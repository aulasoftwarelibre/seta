<table class="table">
    <thead>
        <tr>
            <th>{{ 'label.locker' | trans }}</th>
            <th>{{ 'label.start_at' | trans }}</th>
            <th>{{ 'label.end_at' | trans }}</th>
            <th>{{ 'label.return_at' | trans }}</th>
            <th>{{ 'label.penalized' | trans }}</th>
        </tr>
    </thead>
    <tbody>
    {% for rental in value %}
        <tr>
            <td>{{ rental.locker }}</td>
            <td>{{ rental.startAt | date('d/m/Y') }}</td>
            <td>{{ rental.endAt | date('d/m/Y') }}</td>
            <td>
            {% if rental.returnAt %}
                {{ rental.returnAt | date('d/m/Y') }}
            {% endif %}
            </td>
            <td><span class="label label-info">{{ rental.penalty ? 'SÍ' : 'NO' }}</span> </td>
        </tr>
    {% endfor %}
    </tbody>
</table>