{
    "name": "sergio/seta",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php"                                   : ">=5.5.9",
        "symfony/symfony"                       : "3.0.1",
        "doctrine/orm"                          : "^2.5",
        "doctrine/doctrine-bundle"              : "^1.6",
        "doctrine/doctrine-cache-bundle"        : "^1.2",
        "symfony/swiftmailer-bundle"            : "^2.3",
        "symfony/monolog-bundle"                : "^2.8",
        "sensio/distribution-bundle"            : "^5.0",
        "sensio/framework-extra-bundle"         : "^3.0.2",
        "incenteev/composer-parameter-handler"  : "^2.0",

        "craue/config-bundle"                   : "~1.4",
        "friendsofsymfony/user-bundle"          : "~2.0@dev",
        "hautzi/system-mail-bundle"             : "dev-fix-request-stack",
        "knplabs/knp-menu-bundle"               : "~2.1,>=2.1.1",
        "javiereguiluz/easyadmin-bundle"        : "~1.0"
    },
    "require-dev": {
        "sensio/generator-bundle"               : "^3.0",
        "symfony/phpunit-bridge"                : "^2.7",

        "behat/behat"                           : "~3.1@dev",
        "behat/mink"                            : "~1.7@dev",
        "behat/mink-browserkit-driver"          : "~1.3,>=1.3.1",
        "behat/mink-extension"                  : "~2.1@dev",
        "behat/symfony2-extension"              : "~2.1,>=2.1.1",
        "knplabs/rad-fixtures-load"             : "~1.1@dev",
        "phpspec/phpspec"                       : "~2.4",
        "phpunit/phpunit"                       : "~5.1",
        "henrikbjorn/phpspec-code-coverage"     : "~2.0",
        "sgomez/debug-swiftmailer-bundle"       : "dev-master"

    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sgomez/SystemMailBundle.git"
        }
    ]
}
